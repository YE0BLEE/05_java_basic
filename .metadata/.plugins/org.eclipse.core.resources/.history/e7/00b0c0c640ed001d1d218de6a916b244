package com.lec.ex02_board;

public class BoardVO {
	
	private int bno;
	private String subject;
	private String writer;
	private String content;
	private String crtdate;
	private int readcnt;
	
	public BoardVO() {
		this.bno = 0;
		this.subject = null;
		this.writer = null;
		this.content = null;
		this.crtdate = null;
		this.readcnt = 0;
	}

}
