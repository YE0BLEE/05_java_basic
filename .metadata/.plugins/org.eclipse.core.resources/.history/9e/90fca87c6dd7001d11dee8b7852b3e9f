package exercise;

public class Ex01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// Java02_ex01
		int x = 10;
		int y = 20;
		int z=(++x) + (y--);
		System.out.println(z);  // 11 + 20 = 31
		
		int score = 85;
		String result = (!(score>90)) ? "가" : "나";
		System.out.println(result);

	}

}
